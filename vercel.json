{
    "version": 2,
    "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/index.js"
      }
    ]
  }
  