{
    "version": 2,
    "builds": [
        {
          "src": "./src/index.ts",
          "use": "@vercel/node",
          "config": {
            "includeFiles": ["src/**"]
          }
        }
      ],
    "buildCommand": "npm install -f && npm run build",
    "installCommand": "npm install -f",
    "outputDirectory": "dist",
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./src/index.ts"
      }
    ]
  }
  